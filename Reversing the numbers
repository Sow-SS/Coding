import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int x = sc.nextInt();
	    int rev = 0;
	    int rem;
	    while(x!=0){
	        rem = x%10;
	        rev = (rev * 10) + rem;
	        x /= 10;
	    }
		System.out.println(rev);
	}
}

import java.util.*;
public class Main
{
	public static void main(String[] args) {           // 1
	    Scanner sc = new Scanner(System.in);           // 22
	    int n = sc.nextInt();                          // 333
	    for(int i=1;i<=n;i++){                        //  4444
	        for(int j=1;j<=i;j++){                    //  55555
	            System.out.print(i);
	        }
	        System.out.println();
	    }
	}
}

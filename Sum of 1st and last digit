import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int x = sc.nextInt();
	    int sum;
	    int rem = x%10;
	    while(x!=1){
	        int curr = x/10;
	        x = curr;
	    }
	    sum = rem + x;
		System.out.println(sum);
	}
}

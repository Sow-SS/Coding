import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner (System.in);
	    int n = sc.nextInt();
	    int arr[] = new int[n];
	    int count = 0;
	    for(int i=0;i<n;i++){
	        arr[i] = sc.nextInt();
	    } 
	    int x = sc.nextInt();
	    for(int i=0;i<n;i++){
	        if(arr[i] == x){
	            count+=1;
	        }
	    }
		System.out.println(count);
	}
}

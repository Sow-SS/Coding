import java.util.*;
public class Main
{
	public static void main(String[] args) {           // 12345
	    Scanner sc = new Scanner(System.in);           // 1234
	    int n = sc.nextInt();                          // 123
	    for(int i=1;i<=n;i++){                        //  12
	        for(int j=1;j<=n-i+1;j++){                //  1
	            System.out.print(j);
	        }
	        System.out.println();
	    }
	}
}
